            <div class="col-md-10 bhoechie-tab">

                <!--///////////////////////////MISSION/VISION//////////////////////////////////-->
                <div class="bhoechie-tab-content active" ng-controller="MissionVisionMaintenanceController as vmMissionVision">
                    <h3>Mission/Vision Maintenance</h3>               
                    <div class="col-md-12">
                        <hr />

                        <div class="col-md-10" id="mission_vision_div">
                            <form ng-submit="vmMissionVision.saveMissionVisionOnSubmit(vmMissionVision.isMission)">
                                <h3>
                                    <span ng-bind="vmMissionVision.missionVisionTitle"></span> <span style="color: red;">*</span>
                                </h3>
                                <div class="form-group">
                                    <textarea rows="6"
                                              required="required"
                                              placeholder="Description"
                                              class="form-control"
                                              ng-model="vmMissionVision.missionVisionDescription"></textarea>
                                </div>
                                <button class="btn btn-primary" id="btn-save-mission-vision"><span class="glyphicon-check"></span> Save</button>
                                <button class="btn btn-danger" id="btn-close-mission-vision" type="button" ng-click="vmMissionVision.closeMissionVisionOnClick()">Close</button>
                            </form>                      
                        </div>
                    </div>
                         
                    <div class="col-md-12">
                        <hr />                        

                        <div class="col-md-10">
                            <h3>Mission</h3>
                            <p id="mission_details">
                                <span ng-bind="vmMissionVision.missionDetails"></span>
                            </p>                            
                            <button class="btn btn-warning" ng-click="vmMissionVision.editMissionOnClick(1)" id="btn-mission-edit"><span class="glyphicon-pencil"></span> Edit</button>
                        </div>
                    </div>

                    <div class="col-md-12">
                        <hr />
                        <div class="col-md-10">
                            <h3>Vision</h3>
                            <p id="vision_details">
                                <span ng-bind="vmMissionVision.visionDetails"></span>
                            </p>                            
                            <button class="btn btn-warning btn-mission-vision-edit" ng-click="vmMissionVision.editMissionOnClick(2)" id="btn-vision-edit"><span class="glyphicon-pencil"></span> Edit</button>
                        </div>
                    </div>

                </div>


                <!--///////////////////////////ANNOUNCEMENTS//////////////////////////////////-->
                <div class="bhoechie-tab-content" ng-controller="AnnouncementMaintenanceController as vmAnnouncement">
                    <h3>Announcement Maintenance</h3>
                    <div class="col-md-offset-11">
                        <button class="btn btn-primary" ng-click="vmAnnouncement.announcementOnClick(1, announcement.id)"><span class="glyphicon-plus"> Add</span></button>
                    </div>

                    <div class="col-md-12">
                        <hr />

                        <div class="col-md-10" id="announcement_div">
                            <form ng-submit="vmAnnouncement.announcementOnSubmit(vmAnnouncement.announcementForm.announcementId)">

                                <div class="form-group">
                                    <input type="text" placeholder="Announcement Title" 
                                           class="form-control" ng-model="vmAnnouncement.announcementForm.title"
                                           required="required">
                                </div>

                                <div class="form-group">
                                    <textarea rows="6" placeholder="Description" class="form-control" 
                                              name="" id="" ng-model="vmAnnouncement.announcementForm.description"></textarea>
                                </div>

                                <div class="col-md-12">
                                    <label>Duration</label>
                                    <div class="form-group">
                                        <div class="col-md-3">
                                            <input type="number" placeholder="Day(s)" 
                                                   class="form-control" name="" id="" min="0" ng-model="vmAnnouncement.announcementForm.durationDay" 
                                                   required="required">
                                        </div>

                                        <div class="col-md-3">
                                            <input type="number" placeholder="Hours(s)" 
                                                   class="form-control" name="" id="" min="0" ng-model="vmAnnouncement.announcementForm.durationHour"
                                                   required="required">
                                        </div>
                                        <br />
                                    </div>
                                </div>

                                <div>
                                    <button type="submit" class="btn btn-primary">Save</button>
                                    <button type="button" class="btn btn-danger" 
                                            data-dismiss="modal" ng-click="vmAnnouncement.closeDivOnClick()">Close</button>
                                </div>
                            </form>
                        </div>
                    </div>

                    <div class="col-md-12" ng-repeat="(key, value) in vmAnnouncement.announcements">
                        <hr />
                        <div class="col-md-2">
                            <div class="">
                                <img src="~/images/childrens-park-646x401.jpg" alt="" style="height:100px; width:100px; margin-top:25px">
                            </div>
                        </div>

                        <div class="col-md-10">
                            <h3 ng-bind="value.title"></h3>
                            <p ng-bind="value.description"></p>
                            <button class="btn btn-warning" ng-click="vmAnnouncement.announcementOnClick(2, key)">
                                <span class="glyphicon-pencil"></span> Edit
                            </button>
                            <button class="btn btn-danger" ng-click="vmAnnouncement.announcementOnClick(3, key)"><span class="glyphicon-minus"></span> Delete</button>
                        </div>
                    </div>
                </div>


                <!--///////////////////////////EVENTS//////////////////////////////////-->
