                    <div class="col-md-12" id="logo_div">
                        <hr />
                            <h3><span class="ace-icon fa fa-glass"></span>Logo Informations</h3>
                            
                            <section id="contact-section" style="padding:0px">
                                <div class="block">
                                    <div class="contact-form">
                                        <form id="contact-form" ng-submit="vmLogo.saveImageOnClick()" role="form">
